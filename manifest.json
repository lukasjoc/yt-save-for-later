{
	"name": "yt-save-for-later",
	"version": "0.0.1",
	"description": "Save any youtube video with the current timestamp for later",
  "permissions": [
		"storage"
	],
	"background": {
 		"scripts": [
			"background.js"
		],
   	"persistent": false
   },
   "page_action": {
   	"default_popup": "extension.html"
   },
	"manifest_version": 2
}
